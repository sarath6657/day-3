public class NumberCheck {
    public static void main(String[] args) {
        int[] a = {10, 20, 30, 40, 50};
        int n=40;
        boolean flag = false;
        for (int i=0;i<a.length;i++) {
            if (a[i]==n) {
                flag = true;
            }
        }
                
                if (flag==true) {
            System.out.println("found");
        } else {
            System.out.println("not found");
        }
    }
}
